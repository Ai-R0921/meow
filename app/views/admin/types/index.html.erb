<div class="container">
  <div class="row text-center mt-5">
    <div class="col mx-auto admin-subtitle">
      <h2>猫種</h2>
      <%= link_to "新規登録", new_admin_type_path, class:"btn btn-outline-primary rounded-pill" %>
    </div>
  </div>
  <div class="row mt-3">
    <div class="col-6 mx-auto">
      <table class="table table bordered">
        <tbody>
          <% @types.each do |type| %>
          <tr>
            <td><%= type.name %></td>
            <td><%= link_to "削除", admin_type_path(type), method: :delete, class:"btn btn-outline-danger rounded-pill" %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
