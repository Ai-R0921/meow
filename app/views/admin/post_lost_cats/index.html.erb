<h1>迷子猫一覧</h1>
<% @post_lost_cats.each do |post_lost_cat| %>
  <%= post_lost_cat.title %><br>
  <%= link_to admin_post_lost_cat_path(post_lost_cat) do %>
    <% if post_lost_cat.image.attached? %>
      <%= image_tag post_lost_cat.image, :size => '200x200' %><br>
    <% end %>
  <% end %>
  <%= post_lost_cat.user.user_name %>
<% end %>
